@model List<RPPP_WebApp.Model.Owner>

<h1>Test Database Connection</h1>

@if (Model != null && Model.Count > 0) {
  <table>
    <thead>
      <tr>
        <th>Oib</th>
        <th>Name</th>
        <th>Surname</th>
        <!-- Add more columns as per your table structure -->
      </tr>
    </thead>
    <tbody>
      @foreach (var owner in Model) {
        <tr>
          <td>@owner.Oib</td>
          <td>@owner.Name</td>
          <td>@owner.Surname</td>
          <!-- Display other columns similarly -->
        </tr>
      }
    </tbody>
  </table>
}
else {
  <p>No data found.</p>
}
